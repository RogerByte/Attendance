<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<fx:root type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <StackPane blendMode="MULTIPLY" layoutY="-1.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label text="Horarios" textFill="#008287" wrapText="true" StackPane.alignment="TOP_LEFT">
               <font>
                  <Font name="Segoe UI Semilight" size="20.0" />
               </font>
               <StackPane.margin>
                  <Insets left="10.0" top="10.0" />
               </StackPane.margin>
            </Label>
            <Line endX="95.0" stroke="#008287" strokeWidth="1.5" StackPane.alignment="TOP_LEFT">
               <StackPane.margin>
                  <Insets left="10.0" top="38.0" />
               </StackPane.margin>
            </Line>
            <VBox spacing="10.0">
               <children>
                  <HBox alignment="BOTTOM_RIGHT" spacing="10.0">
                     <StackPane.margin>
                        <Insets left="10.0" right="10.0" top="50.0" />
                     </StackPane.margin>
                     <children>
                        <Button fx:id="btnAgregarHorario" defaultButton="true" mnemonicParsing="false" onAction="#btnAgregarHorario_OnAction" text="Agregar Horario" />
                     </children>
                     <VBox.margin>
                        <Insets top="50.0" />
                     </VBox.margin>
                  </HBox>
               </children>
               <StackPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </StackPane.margin>
            </VBox>
            <TableView fx:id="GridHorarios">
              <columns>
                  <TableColumn fx:id="tcEdit" editable="false" prefWidth="25.0" resizable="false" sortable="false" />
                  <TableColumn fx:id="tcDelete" editable="false" prefWidth="25.0" resizable="false" sortable="false" />
                <TableColumn fx:id="tcIdHorario" editable="false" prefWidth="37.0" text="ID" />
                <TableColumn fx:id="tcDescripcionHoriario" editable="false" prefWidth="93.0" text="Horario" />
                  <TableColumn fx:id="tcLunes" editable="false" minWidth="80.0" text="Lunes" />
                  <TableColumn fx:id="tcMartes" minWidth="80.0" text="Martes" />
                  <TableColumn fx:id="tcMiercoles" minWidth="80.0" text="Miercoles" />
                  <TableColumn fx:id="tcJueves" minWidth="80.0" text="Jueves" />
                  <TableColumn fx:id="tcViernes" minWidth="80.0" text="Viernes" />
                  <TableColumn fx:id="tcSabado" minWidth="80.0" text="Sabado" />
                  <TableColumn fx:id="tcDomingo" minWidth="80.0" text="Domingo" />
              </columns>
               <StackPane.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="100.0" />
               </StackPane.margin>
            </TableView>
         </children>
      </StackPane>
   </children>
</fx:root>
