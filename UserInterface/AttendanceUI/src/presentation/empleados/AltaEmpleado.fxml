                  </HBox>
                  <HBox alignment="CENTER_RIGHT" spacing="10.0">
                     <children>
                        <Label text="ContraseÃ±a:" />
                        <PasswordField fx:id="txtPassword" prefWidth="200.0" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_RIGHT" spacing="10.0">
                     <children>
                        <Label text="Correo:" />
                        <TextField fx:id="txtCorreo" prefWidth="200.0" />
                     </children>
                  </HBox>
                  <HBox alignment="BOTTOM_RIGHT" prefHeight="50.0" spacing="10.0">
                     <children>
                        <Button fx:id="btnCancelar" cancelButton="true" mnemonicParsing="false" onAction="#btnCancelar_OnAction" text="Cancelar" />
                        <Button fx:id="btnAceptar" defaultButton="true" mnemonicParsing="false" onAction="#btnAceptar_OnAction" text="Aceptar" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
		</StackPane>
	</children>
