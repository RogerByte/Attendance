                     </children>
                  </HBox>
                  <HBox alignment="CENTER_RIGHT" prefWidth="200.0" spacing="10.0">
                     <children>
                        <CheckBox fx:id="chkIsManager" mnemonicParsing="false" prefHeight="25.0" prefWidth="200.0" text="Es Manager">
