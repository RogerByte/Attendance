<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.paint.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>

<fx:root prefWidth="358.0" type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
	<children>
      		<StackPane blendMode="MULTIPLY" layoutY="-1.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <Label text="Información Directorio" textFill="#008287" StackPane.alignment="TOP_LEFT">
               <font>
                  <Font size="18.0" />
               </font>
               <StackPane.margin>
                  <Insets left="10.0" top="10.0" />
               </StackPane.margin>
            </Label>
            <VBox prefWidth="100.0" spacing="10.0" StackPane.alignment="TOP_LEFT">
               <StackPane.margin>
                  <Insets bottom="10.0" left="15.0" right="15.0" top="50.0" />
               </StackPane.margin>
               <children>
                  <HBox alignment="TOP_RIGHT" prefWidth="200.0" spacing="10.0">
                     <children>
                        <Label prefHeight="25.0" text="Numero Extensión:" />
                        <TextField fx:id="txtNumExt" prefWidth="200.0" />
                     </children>
                  </HBox>
                  <HBox alignment="TOP_RIGHT" prefWidth="200.0" spacing="10.0">
                     <children>
                        <Label prefHeight="25.0" text="Ubicación:" />
                        <ChoiceBox fx:id="cmbUbicacion" prefWidth="200.0" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_RIGHT" spacing="10.0">
                     <children>
                        <Label text="Celular:" />
                        <TextField fx:id="txtCorreo" prefWidth="200.0" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER_RIGHT" spacing="10.0">
                     <children>
                        <Button mnemonicParsing="false" prefWidth="70.0" text="Foto" />
                        <Label prefWidth="200.0" />
                     </children>
                  </HBox>
                  <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
                     <children>
                        <ImageView fitHeight="150.0" fitWidth="120.0" pickOnBounds="true" preserveRatio="true" />
                     </children>
                  </HBox>
                  <HBox alignment="BOTTOM_RIGHT" prefHeight="50.0" spacing="10.0">
                     <children>
                        <Button fx:id="btnCancelar" cancelButton="true" mnemonicParsing="false" onAction="#btnCancelar_OnAction" text="Cancelar" />
                        <Button fx:id="btnAceptar" defaultButton="true" mnemonicParsing="false" onAction="#btnAceptar_OnAction" text="Aceptar" />
                     </children>
                  </HBox>
               </children>
            </VBox>
         </children>
		</StackPane>
	</children>
</fx:root>
